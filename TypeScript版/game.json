{
	"width": 1024,
	"height": 512,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"systems": {
			"type": "script",
			"path": "script/systems.js",
			"global": true
		},
